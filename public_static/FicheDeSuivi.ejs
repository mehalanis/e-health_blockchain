 <!DOCTYPE html>
 <html>
 
 <head id="head">
   <meta charset="utf-8">
   <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
   <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
   <meta name="author" content="Creative Tim">
   <title></title>
   <!-- Favicon -->
   <link rel="icon" href="/assets/img/brand/favicon.png" type="image/png">
   <!-- Fonts -->
   <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
   <!-- Icons -->
   <link rel="stylesheet" href="/assets/vendor/nucleo/css/nucleo.css" type="text/css">
   <link rel="stylesheet" href="/assets/vendor/@fortawesome/fontawesome-free/css/all.min.css" type="text/css">
   <!-- Page plugins -->
   <!-- Argon CSS -->
   <link rel="stylesheet" href="/assets/css/argon.css?v=1.1.0" type="text/css">
 </head>
 
 <body>
   <!-- Sidenav -->
   <nav class="sidenav navbar navbar-vertical fixed-left navbar-expand-xs navbar-light bg-white" id="<%if(user_type=='patient'){ %>sidenav-main<% }else{  %>sidenav-main_medecin<% }%>">
     
   </nav>
   <!-- Main content -->
   <div class="main-content" id="panel">
     <!-- Topnav -->
     <nav class="navbar navbar-top navbar-expand navbar-dark bg-primary border-bottom" id="navbar">
       
     </nav>
     <!-- Header -->
     <!-- Header -->
     <div class="header bg-primary pb-6">
       <div class="container-fluid">
         <div class="header-body">
           <div class="row align-items-center py-4">
             <div class="col-lg-6 col-7">
               <h6 class="h2 text-white d-inline-block mb-0">fichier de suivi</h6>
               <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                 <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                   <li class="breadcrumb-item"><a href="#"><i class="fas fa-home"></i></a></li>
                   <li class="breadcrumb-item"><a href="#">Dashboards</a></li>
                   <li class="breadcrumb-item active" aria-current="page">fichier de suivi</li>
                 </ol>
               </nav>
             </div>
             <%if(user_type!='patient'){ %>
             <div class="col-lg-6 col-5 text-right">
              <a href="/ADDFicheDeSuivi/<%=patinet_id%>_<%= dossier_id %>" class="btn btn-sm btn-neutral">New</a>
              <a href="#" class="btn btn-sm btn-neutral">Filters</a>
            </div>
            <%}%>
           </div>
          </div>
        </div>
      </div>

      <div class="container-fluid mt--6">
        <!-- Table -->
        <div class="row">
          <div class="col">
            <div class="card">
                <div class="card-header bg-transparent">
                  <h3 class="mb-0">Timeline</h3>
                </div>
                <div class="card-body">
                  <div class="timeline timeline-one-side" data-timeline-content="axis" data-timeline-axis-style="dashed">
                    <% for(var i=dossier.length-1; i>=0;i--){ 
                      dArr =dossier[i].DataCreation.split("-");
                      %>
                    <div class="timeline-block">
                      <span class="timeline-step badge-info">
                        <i class="ni ni-like-2"></i>
                      </span>
                      <div class="timeline-content">
                        <small class="text-muted font-weight-bold"><%= dArr[2]+ "/" +dArr[1]+ "/" +dArr[0] %></small>
                        <p lass=" text-sm mt-1 mb-0"> Type Malade: <%= dossier[i].Type %>  </p>
                        <h5 class=" mt-3 mb-0">Observation</h5>
                        <p class=" text-sm mt-1 mb-0"><%= dossier[i].Observation %></p>
                        <h5 class=" mt-3 mb-0">Compte Rendu</h5>
                        <p class=" text-sm mt-1 mb-0"><%= dossier[i].CompteRendu %></p>
                      </div>
                    </div>
                    <% } %>
                  </div>
                </div>
              </div>
          </div>
        </div>
       <!-- Footer -->
       <footer class="footer pt-0" id="footer">
         
       </footer>
     </div>
   </div>
   <!-- Argon Scripts -->
   <!-- Core -->
   <script src="/assets/vendor/jquery/dist/jquery.min.js"></script>
   <script src="/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
   <script src="/assets/vendor/js-cookie/js.cookie.js"></script>
   <script src="/assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
   <script src="/assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
   <!-- Optional JS -->
   <script src="/assets/vendor/chart.js/dist/Chart.min.js"></script>
   <script src="/assets/vendor/chart.js/dist/Chart.extension.js"></script>
   <!-- Argon JS -->
   <script src="/assets/js/argon.js?v=1.1.0"></script>
   <!-- Demo JS - remove this in your project -->
   <script src="/assets/js/demo.min.js"></script>
   
   <script src="/javascript/index.js"></script>
 </body>
 
 </html>