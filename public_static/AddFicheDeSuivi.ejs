 <body>
   <!-- Sidenav -->
   <nav class="sidenav navbar navbar-vertical fixed-left navbar-expand-xs navbar-light bg-white" id="sidenav-main">
     
   </nav>
   <!-- Main content -->
   <div class="main-content" id="panel">
     <!-- Topnav -->
     <nav class="navbar navbar-top navbar-expand navbar-dark bg-primary border-bottom" id="navbar">
       
     </nav>
     <!-- Header -->
     <!-- Header -->
     <div class="header bg-primary pb-6">
       <div class="container-fluid">
         <div class="header-body">
           <div class="row align-items-center py-4">
             <div class="col-lg-6 col-7">
               <h6 class="h2 text-white d-inline-block mb-0">fichier de suivi</h6>
               <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                 <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                   <li class="breadcrumb-item"><a href="#"><i class="fas fa-home"></i></a></li>
                   <li class="breadcrumb-item"><a href="#">Dashboards</a></li>
                   <li class="breadcrumb-item active" aria-current="page">fichier de suivi</li>
                 </ol>
               </nav>
             </div>
            
           </div>
          </div>
        </div>
      </div>

      <div class="container-fluid mt--6">
        <!-- Table -->
        <div class="row">
          <div class="col">
            <div class="card">
                <!-- Card header -->
                <div class="card-header">
                  <h3 class="mb-0">Ajouter d'une fichier de suivi</h3>
                </div>
                <!-- Card body -->
                <div class="card-body">
                  <form method="POST" action="/ADDFicheDeSuivi">
                    <!-- Input groups with icon -->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                              <div class="input-group input-group-merge">
                                <div class="input-group-prepend">
                                  <span class="input-group-text"><i class="fas fa-user"></i></span>
                                </div>
                                <input class="form-control" placeholder="Type Maladie" type="text" name="Type">
                              </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                        <div class="form-group">
                          <div class="input-group input-group-merge">
                            <div class="input-group-prepend">
                              <span class="input-group-text"><i class="fas fa-user"></i></span>
                            </div>
                            <input class="form-control" placeholder="Data" type="date" name="DataCreation">
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                              <div class="input-group input-group-merge">
                                <textarea class="form-control" name="Observation" placeholder="Observation ..."  cols="30" rows="3"></textarea>
                              </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                      <div class="col-md-12">
                        <div class="form-group">
                          <div class="input-group input-group-merge">
                            <textarea class="form-control" name="CompteRendu" placeholder="Compte Rendu ..." cols="30" rows="3"></textarea>
                          </div>
                        </div>
                      </div>
                    </div>
                    <input type="hidden" name="user_id" value="<%= user_id %>">
                    <input type="hidden" name="dossier_id" value="<%= dossier_id %>">
                    <!-- Input groups with icon -->
                    <button class="btn btn-primary" type="submit">Submit form</button>
                  </form>
                </div>
              <div>
              </div>
            </div>
          </div>
        </div>
       <!-- Footer -->
       <footer class="footer pt-0" id="footer">
         
       </footer>
     </div>
   </div>
   <!-- Argon Scripts -->
   <!-- Core -->
   <script src="/assets/vendor/jquery/dist/jquery.min.js"></script>
